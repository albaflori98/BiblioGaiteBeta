<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout7"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    tools:context=".ClasesPropias.Proyectos">

    <TextView
        android:id="@+id/tituloProyecto"
        android:layout_width="131dp"
        android:layout_height="151dp"
        android:background="#FEFEFE"
        android:fontFamily="@font/montserrat_bold"
        android:gravity="bottom"
        android:onClick="botonProyectosPulsado"
        android:text="PROYECTOS DE FIN DE CICLO"
        android:textAlignment="center"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imagenProyecto"
        android:layout_width="65dp"
        android:layout_height="86dp"
        android:onClick="botonProyectosPulsado"
        android:src="@drawable/proyecto"
        app:layout_constraintBottom_toBottomOf="@+id/tituloProyecto"
        app:layout_constraintEnd_toEndOf="@+id/tituloProyecto"
        app:layout_constraintHorizontal_bias="0.508"
        app:layout_constraintStart_toStartOf="@+id/tituloProyecto"
        app:layout_constraintTop_toTopOf="@+id/tituloProyecto" />

    <Space
        android:layout_width="132dp"
        android:layout_height="23dp"
        tools:layout_editor_absoluteX="5dp"
        tools:layout_editor_absoluteY="145dp" />

</androidx.constraintlayout.widget.ConstraintLayout>